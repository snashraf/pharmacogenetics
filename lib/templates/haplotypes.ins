INSERT INTO Haplotypes VALUES(
	"{{json.gene.id}}",	
	"{{json.id}}",
	"{{json.symbol}}",
	"{{json.name}}",
	"{{json.hgvs}}"
);

{% for doc in json.alleles %}
	{% if doc.allele %}
		INSERT INTO HapVars VALUES(
			"{{json.id}}",
			"{{doc.location.displayName}}",
			"{{doc.allele}}"
		);
	{% endif %}
{% endfor %}
