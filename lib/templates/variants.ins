INSERT INTO Variants VALUES(
	{{json.id}},
	{{json.name}},
	{{json.clinicalSignificance}}
);
{% for doc in json.location %}
	{% if "ChGR37" in doc.name %}
	
		INSERT INTO LocPGKB VALUES(
			{{json.id}},
			{{doc.name}},
			{{doc.begin}},
			{{doc.end}},
			{{doc.reference}}
		);
	
	{% endif %}
{% endfor %}

{% for var in json.variants %}
	
	INSERT INTO AltAlleles VALUES(
		{{json.id}},
		NULL,
		{{var}}
	);
	
{% endfor %}